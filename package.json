{
    "private": true,
    "name": "public-website",
    "version": "0.1.0",
    "scripts": {
        "build": "react-scripts build",
        "ci_format": "prettier-eslint --list-different $PWD/\"**/*.js\" $PWD/\"**/*.jsx\" && eslint \"**/*.js\" \"**/*.jsx\" && prettier-package-json  --tab-width 4 ./package.json",
        "eject": "react-scripts eject",
        "format": "prettier-eslint $PWD/\"**/*.js\" $PWD/\"**/*.jsx\" --write && eslint \"**/*.js\" \"**/*.jsx\" --fix && prettier-package-json --write --tab-width 4 ./package.json",
        "lint": "eslint \"**/*.js\" \"**/*.jsx\" --fix",
        "start": "react-scripts start",
        "test": "react-scripts test"
    },
    "dependencies": {
        "ajv": "^6.10.0",
        "bootstrap": "^4.3.1",
        "node-sass": "^4.13.1",
        "react": "next",
        "react-bootstrap": "^1.0.0-beta.9",
        "react-dom": "next",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.0.5"
    },
    "devDependencies": {
        "prettier-eslint": "^9.0.1",
        "prettier-eslint-cli": "^5.0.0",
        "prettier-package-json": "^2.1.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "react-app"
    }
}
